<% provide(:title, 'All Users') %>
<h1>All Users</h1>
<%= will_paginate %>
<ul class="resources">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user %>
    </li>
  <% end %>
</ul>
<%= will_paginate %>

<%= link_to "Create User", users_new_path, class: "btn btn-lg btn-primary" %>