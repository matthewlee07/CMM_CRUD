<% provide(:title, 'All Projects') %>
<h1>All Projects</h1>
<%= render 'shared/paginate', :@profile => @projects%>
  <ul class="resources">
    <% @projects.each do |project| %>
      <li> 
        <%= link_to project.project_name, project %>
      </li>
    <% end %>
  </ul>
<%= render 'shared/paginate', :@profile => @projects%>
<div class="center">
  <%= link_to "Create Project", projects_new_path, class: "btn btn-lg btn-primary" %>
</div>
